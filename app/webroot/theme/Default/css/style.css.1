/** Forms **/
form {
	clear: both;
	margin-right: 20px;
	padding: 3px 8px;;
	width: 98%;
	border: 1px solid black;
	background-color: #ccc;
}
div.search { margin-bottom: 10px; }
div.search form { background-color: transparent; border: none; }
form div {
	clear: both;
	margin-bottom: 2px;
	padding: .1em;
	vertical-align: text-top;
}
form .input { color: #444; }
form .required { font-weight: bold; }
form .required label:after {
	color: #e32;
	content: '*';
	display:inline;
}
form div.submit {
	border: 0;
	clear: both;
	margin-top: 10px;
}
label {
	display: inline-block;
	font-size: 100%;
	margin-bottom:3px;
	width: 200px;
}
input, textarea {
	font-size: 100%;
	font-family: "frutiger linotype", "lucida grande", "verdana", sans-serif;
	padding: 6px;
    border:1px solid #96a6c5;
}
input { width:160px; }
textarea { width:460px; }
select { font-size: 100%; /*	vertical-align: text-bottom;*/ padding:4px; border: 1px solid #96a6c5; padding: 6px 6px 5px; }
select[multiple=multiple] { width: 100%; }
option { font-size: 100%; padding: 0 3px; }
input[type=checkbox] {
	clear: left;
	float: left;
	margin: 0px 6px 7px 2px;
	width: auto;
}
div.checkbox label { display: inline; }
input[type=radio] {
	float:left;
	width:auto;
	margin: 6px 0;
	padding: 0;
	line-height: 26px;
}
.radio label { margin: 0 0 6px 20px; line-height: 26px; }
input[type=submit], button[type=submit], button[type=reset], .btnCancel { 
	font-family: "frutiger linotype", "lucida grande", "verdana", sans-serif;
	display: inline; 
	font-weight: bold; 
    border: none;
    font-size: 16px;
    color: #f4f4f4;
    border-radius: 5px;
    padding: 4px 8px 6px;
    cursor: pointer;
	margin: 5px 2px;
}
input.btnLogin { background: #2e2e2e; }
input.btnLogin:hover { background: #1c1c1c; }

button[type=submit] { background: #92b6dc; }
button[type=submit]:hover { background: #84a5c7; }

button[type=reset]  { background: #6fb586; }
button[type=reset]:hover { background: #4fb586; }

.btnCancel { background: #ad1329; }
.btnCancel:hover { background: #961024; }

form .submit input[type=submit]:hover { background: #5BA150; }
/* Form errors */
form .error {
	background: #FFDACC;
	-moz-border-radius: 4px;
	-webkit-border-radius: 4px;
	border-radius: 4px;
	font-weight: normal;
	padding-top:4px;
}
form .error-message {
	-moz-border-radius: none;
	-webkit-border-radius: none;
	border-radius: none;
	border: none;
	background: none;
	margin: 0;
	padding-left: 204px;
	padding-right: 0;
}
form .error,
form .error-message {
	color: #9E2424;
	-webkit-box-shadow: none;
	-moz-box-shadow: none;
	-ms-box-shadow: none;
	-o-box-shadow: none;
	box-shadow: none;
	text-shadow: none;
}

#dd, #mm, #yy { width: 80px; }
#pencarian #btnSearch { width: 80px; }

