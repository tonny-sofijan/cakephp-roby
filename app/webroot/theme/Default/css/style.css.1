.tableWrapper table { border: 1px solid #222; margin-bottom: 5px; }
.tableWrapper table tbody tr:nth-child(odd) { background: #C5C5C5; }
.tableWrapper table tbody tr:nth-child(even) { background: #D5D5D5; }
